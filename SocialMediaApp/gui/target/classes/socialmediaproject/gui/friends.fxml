<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="445.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialmediaproject.gui.friendsController">
   <children>
      <AnchorPane layoutX="301.0" prefHeight="400.0" prefWidth="206.0">
         <children>
            <Button fx:id="addFriendButton" layoutX="55.0" layoutY="52.0" mnemonicParsing="false" onAction="#onAddFriend" style="-fx-background-color: #2b2b2b;" text="Add friend" textFill="WHITE" />
         </children></AnchorPane>
      <TextField fx:id="searchBar" layoutX="47.0" layoutY="54.0" prefHeight="25.0" prefWidth="270.0" promptText="Search" />
      <Label layoutX="48.0" layoutY="16.0" prefHeight="25.0" prefWidth="230.0" text="Search for user to add friend:">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextArea fx:id="friendList" editable="false" layoutX="47.0" layoutY="166.0" prefHeight="200.0" prefWidth="200.0" wrapText="true" />
      <Label layoutX="48.0" layoutY="123.0" prefHeight="25.0" prefWidth="230.0" text="Your friend list:">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="stateLabel" layoutX="166.0" layoutY="98.0" prefHeight="25.0" prefWidth="270.0">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
